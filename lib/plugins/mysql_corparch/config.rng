<?xml version="1.0" encoding="utf-8"?>
<grammar xmlns="http://relaxng.org/ns/structure/1.0"
         datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
    <start>
        <element name="corparch">
            <element name="module">
                <value>mysql_corparch</value>
            </element>
            <element name="js_module">
                <choice>
                    <value>ucnkCorparch</value>
                    <value>defaultCorparch</value>
                </choice>
            </element>
            <element name="default_page_list_size">
                <data type="positiveInteger" />
            </element>
            <element name="tag_prefix">
                <text />
            </element>
            <element name="max_num_hints">
                <data type="positiveInteger" />
            </element>
            <element name="max_num_favorites">
                <data type="positiveInteger" />
            </element>
            <optional>
                <externalRef href="../common/mysql.rng" />
            </optional>
            <optional>
                <element name="enable_parallel_acc">
                    <choice>
                        <value>false</value>
                        <value>true</value>
                    </choice>
                </element>
            </optional>
        </element>
    </start>
</grammar>